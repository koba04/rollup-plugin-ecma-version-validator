{
  "name": "rollup-plugin-ecma-version-validator",
  "version": "0.2.1",
  "description": "A rollup plugin to verify ECMAScript version for bundle files",
  "engines": {
    "node": ">=14"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "eslint 'src/**/*.ts'",
