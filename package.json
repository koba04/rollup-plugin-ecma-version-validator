    "start": "tsc -w",
    "test": "jest"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "ecma"
  ],
  "author": "koba04",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/koba04/rollup-plugin-ecma-version-validator.git"
  },
  "homepage": "https://github.com/koba04/rollup-plugin-ecma-version-validator",
  "bugs": "https://github.com/koba04/rollup-plugin-ecma-version-validator/issues",
  "dependencies": {
    "acorn": "^8.7.1"
  },
  "peerDependencies": {
    "rollup": "^2.16.1"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^17.0.1",
    "@types/acorn": "^4.0.6",
    "@types/jest": "^27.5.1",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.74.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  }
